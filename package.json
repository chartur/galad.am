{
  "name": "jewelry-store",
  "version": "0.0.0",
  "engines": {
    "node": "18.15.0",
    "npm": "9.5.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=local",
    "start:external": "ng serve --configuration=local --host 0.0.0.0",
    "start:dev": "ng serve --configuration=development",
    "start:prod": "ng serve --configuration=production",
    "start:ios": "ionic capacitor run ios -l --external --configuration development",
    "start:android": "ionic capacitor run android -l --external --configuration=development",
    "start:ios:prod": "ionic capacitor run ios -l --external --configuration production",
    "start:android:prod": "ionic capacitor run android -l --external --configuration=production",
    "build:ios:prod": "ionic capacitor build ios -l --external --configuration production",
    "build:android:prod": "ionic capacitor build android -l --external --configuration production",
    "build": "ng build --configuration development",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run jewelry-store:serve-ssr",
    "serve:ssr": "node dist/jewelry-store/server/main.js",
    "build:dev:ssr": "ng build --configuration=development && ng run jewelry-store:server",
    "build:prod:ssr": "ng build --configuration=production && ng run jewelry-store:server",
    "prerender": "ng run jewelry-store:prerender",
    "build:ssr": "ng build && ng run jewelry-store:server"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^18.2.5",
    "@angular/animations": "^18.2.5",
    "@angular/common": "^18.2.5",
    "@angular/compiler": "^18.2.5",
    "@angular/core": "^18.2.5",
    "@angular/forms": "^18.2.5",
    "@angular/platform-browser": "^18.2.5",
    "@angular/platform-browser-dynamic": "^18.2.5",
    "@angular/platform-server": "^18.2.5",
    "@angular/router": "^18.2.5",
    "@angular/youtube-player": "^17.0.3",
    "@capacitor/android": "^6.0.0",
    "@capacitor/angular": "^2.0.3",
    "@capacitor/app": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/keyboard": "^6.0.0",
    "@capacitor/screen-orientation": "^6.0.2",
    "@capacitor/splash-screen": "^6.0.0",
    "@capacitor/status-bar": "^6.0.0",
    "@ionic/angular": "^7.6.0",
    "@ngrx/component-store": "^17.0.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "angular-star-rating": "^6.0.0",
    "bootstrap": "^5.2.3",
    "css-star-rating": "^1.3.1",
    "emulator": "0.1.0",
    "express": "^4.18.2",
    "localstorage-polyfill": "^1.0.1",
    "lodash": "^4.17.21",
    "ngx-bootstrap": "12.0.0",
    "ngx-slider-v2": "^17.0.0",
    "ngx-toastr": "^18.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular/cli": "~18.2.5",
    "@angular/compiler-cli": "^18.2.5",
    "@capacitor/assets": "^3.0.4",
    "@capacitor/cli": "^6.0.0",
    "@ionic/angular-toolkit": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~4.3.0",
    "@types/lodash": "^4.14.202",
    "@types/node": "^14.15.0",
    "browser-sync": "^3.0.2",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~5.5.4"
  }
}
